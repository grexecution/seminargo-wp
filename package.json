{
  "name": "seminargo-theme",
  "version": "1.0.0",
  "description": "Modern WordPress theme for Seminargo",
  "author": "Seminargo Team",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "sass": "sass src/scss/main.scss:assets/css/main.css --watch --style expanded",
    "sass:build": "sass src/scss/main.scss:assets/css/main.css --style compressed",
    "lint:css": "stylelint 'src/scss/**/*.scss'",
    "lint:js": "eslint src/js/**/*.js",
    "format": "prettier --write \"src/**/*.{js,scss,css}\""
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@wordpress/eslint-plugin": "^17.0.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}